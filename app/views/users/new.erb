<div class="container">
  <h1>Create an Account</h1>
<form action="/users" method="POST">
<ul>
  <li>
    <input id="contact_name" type="text" name="user[email]" placeholder = "Email" />
  </li>
  <li>
    <input id="contact_email" type="password" name="user[password]" placeholder = "Password" />
  </li>
  <li>
    <input type="submit" value="Submit">
  </li>
</ul>
</form>
</div>
